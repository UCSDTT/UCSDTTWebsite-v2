{
  "parser": "babel-eslint",
  "extends": ["eslint:recommended", "plugin:react/recommended"],
  "plugins": ["prettier"],
  "rules": {
    "prettier/prettier": "error",
    "no-unused-vars": 0,
    "no-undef": 0,
    "object-curly-spacing": [2, "always"],
    "indent": ["error", 2],
    "max-len": [1, 120, 2, { "ignoreComments": true }],
    "react/prop-types": [0]
  }
}
